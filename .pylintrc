[MESSAGES CONTROL]
# http://docs.pylint.org/features.html#pylint-checkers-options-and-switches
# I0011: Locally disabling warning
# W0511: TODO in comments
# W0403: Relative import %r, should be %r
# C0103: Invalid %s name "%s"
# C0111: Missing %s docstring
# C0302: Too many lines in module (%s)
# R0902: Too many instance attributes (%s/%s)
# R0903: Too few public methods
# R0904: Too many public methods
# R0912: Too many branches (%s/%s)
# R0913: Too many arguments (%s/%s)
# R0914: Too many local variables (%s/%s)
# R0915: Too many statements (%s/%s)
disable=I0011,W0511,W0403,C0103,C0111,C0302,R0902,R0903,R0904,R0912,R0913,R0914,R0915

[REPORTS]
reports=yes
msg-template={msg_id}:{line:3d},{column}:{msg}

[FORMAT]
max-line-length=99
ignore-long-lines=^.*((https?:)|(pragma:)|(TODO:)).*$
indent-string='    '

[BASIC]
good-names=msg,e,f,fd,fp,i,j,k,ex,Run,_
